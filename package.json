  "dependencies": {
    "concurrently": "^6.0.0",
    "express": "^4.17.1",
    "mssql": "^5.1.0",
    "nodemon": "^2.0.7"
  },
  "devDependencies": {
    "json-server": "^0.15.0"
  }
}
